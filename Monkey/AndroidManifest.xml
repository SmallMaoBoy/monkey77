<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.monkey"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Light.NoTitleBar" >
        <activity
            android:name="com.monkey.activity.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity 
            android:name="com.monkey.activity.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.monkey.activity.GuideActivity"
            android:label="@string/app_name" >
        </activity>
         <activity
            android:name="com.monkey.activity.ActivityShowdetails"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.monkey.activity.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.monkey.activity.MyInfoActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
